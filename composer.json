{
    "name": "ajoy39/laravel-storage-azure",
    "description": "An Azure Blob Storage driver for Laravel Storage",
    "type": "library",
    "require": {
        "league/flysystem-azure-blob-storage": "^1.0.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0.0",
        "phpunit/phpunit": "^9.0.0"
    },
    "suggest": {
        "league/flysystem-cached-adapter": "For increased filesystem perfomance when using azure storage"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Alec Joy",
            "email": "alec@alecjoy.com"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "LaravelAzureStorage\\Providers\\AzureBlobStorageProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "LaravelAzureStorage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
